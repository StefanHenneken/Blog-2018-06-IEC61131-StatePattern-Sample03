    <Method Name="wireUpStateTransitions" Id="{ae777051-a0ea-4107-b4e5-d78929e86b79}">
      <Declaration><![CDATA[METHOD PRIVATE wireUpStateTransitions
]]></Declaration>
      <Implementation>
        <ST><![CDATA[fbWaitingState.SetTransitionStates(
	Machine := THIS^,
	CoinInsertedNext := fbHasCoinState
);

fbHasCoinState.SetTransitionStates(
	Machine := THIS^,
	ButtonPressedProductDecrementedNext := fbProductEjectedState,
	ButtonPressedNoProductNext := fbCoinEjectedState
);

fbProductEjectedState.SetTransitionStates(
	Machine := THIS^,
	AfterProductEjectedNext := fbWaitingState
);

fbCoinEjectedState.SetTransitionStates(
	Machine := THIS^,
	CustomerTakesCoinNext := fbWaitingState
);]]></ST>
      </Implementation>
    </Method>
